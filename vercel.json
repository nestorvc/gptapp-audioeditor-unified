{
  "version": 2,
  "builds": [
    {
      "src": "build/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/wp-admin/(.*)",
      "dest": "/api/blocked"
    },
    {
      "src": "/wp-content/(.*)",
      "dest": "/api/blocked"
    },
    {
      "src": "/wp-includes/(.*)",
      "dest": "/api/blocked"
    },
    {
      "src": "/wordpress/(.*)",
      "dest": "/api/blocked"
    },
    {
      "src": "/phpmyadmin/(.*)",
      "dest": "/api/blocked"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/api/blocked"
    },
    {
      "src": "/administrator/(.*)",
      "dest": "/api/blocked"
    },
    {
      "src": "/\\.env",
      "dest": "/api/blocked"
    },
    {
      "src": "/\\.git/(.*)",
      "dest": "/api/blocked"
    },
    {
      "src": "/(.*)",
      "dest": "build/index.js"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}

